{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad?post_login_redirect_uri=.referrer"
    },
    "404": {
      "statusCode": 404,
      "rewrite": "/404.html"
    }
  },
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/a59521c6-1c6e-46ee-8213-3bb9f8d48fe7/v2.0",
          "clientIdSettingName": "LANDINGPAGE_AUTH_CLIENT_ID",
          "clientSecretSettingName": "LANDINGPAGE_AUTH_SECRET"
        }
      }
    }
  }
}